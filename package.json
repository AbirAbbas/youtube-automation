{
  "name": "youtube-automation",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered YouTube content generator with local TTS support (Kokoro) and cloud options (ElevenLabs)",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:env": "source .env.local && npm run dev",
    "build:env": "source .env.local && npm run build",
    "start:env": "source .env.local && npm run start",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "setup:local-tts": "echo 'Installing Kokoro TTS...' && echo 'Run: pip install kokoro>=0.9.2 soundfile && apt-get install espeak-ng' && echo 'For fallback: pip install TTS torch torchaudio' && echo 'Test with: npm run test:kokoro'",
    "test:kokoro": "node test-kokoro-tts.js",
    "test:kokoro:env": "source .env.local && node test-kokoro-tts.js"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.7.0",
    "@google-cloud/local-auth": "^3.0.1",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/pg": "^8.15.4",
    "canvas": "^3.1.2",

    "clsx": "^2.1.1",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "fluent-ffmpeg": "^2.1.3",
    "googleapis": "^154.0.0",
    "next": "15.4.3",
    "openai": "^5.10.2",
    "pexels": "^1.4.0",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "optionalDependencies": {
    "comment": "ElevenLabs is now optional - use local TTS (Coqui) for free RTX 4090 inference"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "youtube",
    "automation",
    "ai",
    "tts",
    "local-tts",
    "kokoro",
    "elevenlabs",
    "video-generation",
    "rtx-4090"
  ]
}
